{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://jckc-app-backend.onrender.com/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "https://jckc-app-backend.onrender.com/uploads/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
